{
    "name": "cogroup/cms",
    "description": "CMS for all laravel projects",
    "keywords" : ["cms", "admin", "laravel","illuminate","auth","roles","permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "COgroup",
            "email": "support@cogroupsas.com",
            "homepage": "https://wwww.cogroupsas.com"
        }
    ],
    "require": {
        "laravel/socialite": "^3",
        "intervention/image": "^2",
        "mailjet/laravel-mailjet": "^2"
    },
    "autoload": {
        "psr-4": {
            "Cogroup\\Cms\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
