{
    "name": "cogroup/cms",
    "description": "CMS for all laravel projects",
    "keywords" : ["cms", "admin", "laravel","illuminate","auth","roles","permission"],
    "license": "MIT",
    "version": "1.7.4",
    "authors": [
        {
            "name": "COgroup",
            "email": "support@cogroupsas.com",
            "homepage": "https://wwww.cogroupsas.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/socialite": "^3.0",
        "intervention/image": "^2.4.1"
    },
    "autoload": {
        "psr-4": {
            "Cogroup\\Cms\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Cogroup\\Cms\\CmsServiceProvider"
            ],
            "aliases": {
                "Cms": "Cogroup\\Cms\\Facades\\Cms"
            }
        }
    },
    "require-dev": {
        "laravel/framework": "5.8.x"
    },
    "config": {
        "sort-packages": true
    }
}
