{
    "name": "cogroup/cms",
    "description": "CMS for all laravel projects",
    "keywords" : ["cms", "admin", "laravel","illuminate","auth","roles","permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "COgroup",
            "email": "support@cogroupsas.com",
            "homepage": "https://cogroupsas.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/socialite": "^3.0",
        "intervention/image": "^2.4.1"
    },
    "autoload": {
        "psr-4": {
            "Cogroup\\Cms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cogroup\\Cms\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.2-dev"
        },
        "laravel": {
            "providers": [
                "Cogroup\\Cms\\CmsServiceProvider"
            ],
            "aliases": {
                "Cms": "Cogroup\\Cms\\Facades\\Cms"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
